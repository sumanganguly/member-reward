{
  "testRegex": ".*unit-test\\.js$",
  "testEnvironment": "node",
  "collectCoverageFrom": ["app/**/*.js"],
  "coveragePathIgnorePatterns": [
    "./node_modules",
    "./coverage",
    "./test",
    "./app/controllers",
    "./app/index.js",
    "./app/controllers",
    "./app/db",
    "./app/libs/model-helper.js",
    "./config"
  ],  
  "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
}
